	<build>
	<plugins>

		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-compiler-plugin</artifactId>
			<version>2.4</version>
			<configuration>
				<source>1.6</source>
				<target>1.6</target>
				<fork>true</fork>
				<meminitial>256m</meminitial>
				<maxmem>512m</maxmem>
			</configuration>
		</plugin>
		
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-dependency-plugin</artifactId>
			<version>2.4</version>
			<configuration>
				<outputDirectory>${webRoot}/WEB-INF/lib/</outputDirectory>
			</configuration>
		</plugin>
		
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-jar-plugin</artifactId>
			<version>2.4</version>
			<configuration>
				<includes>
					<include>**/*.class</include>
					<include>**/*.ftl</include>
					<include>**/META-INF/**</include>
					<include>**/applicationContext-memcache.xml</include>
					<include>**/applicationContext-security.xml</include>
					<include>**/dataSheetConfig.xml</include>
					<include>**/jbpm.cfg.xml</include>
					<include>**/jbpm.mail.properties</include>
					<include>**/jbpm.mail.templates.examples.xml</include>
					<include>**/mmsMessages_zh_CN.properties</include>
					<include>**/wfMessages_zh_CN.properties</include>
					<include>**/threadConfig.properties</include>
					<include>**/publicAutoAuthConfig.xml</include>
					<include>**/struts-plugin.xml</include>
					<include>**/applicationContent.properties</include>
					<include>**/redirectUrl.properties</include>
				</includes>
				<excludes>
					<exclude>**/mytheme/**</exclude>
					<exclude>**/bs/**/web/**</exclude>
					<exclude>**/task/web/**</exclude>
					<exclude>**/portal/web/portlet/**</exclude>
				</excludes>
			</configuration>
		</plugin>
		
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-war-plugin</artifactId>
			<version>2.2</version>
			<configuration>
				<warName>imatrix</warName>
				<warSourceDirectory>${webRoot}</warSourceDirectory>
				<packagingExcludes>
					WEB-INF/lib/junit-*.jar,WEB-INF/lib/unitils-*.jar,WEB-INF/lib/jmockit-*.jar,WEB-INF/lib/servlet-api-*.jar,WEB-INF/lib/jsp-api-*.jar,WEB-INF/lib/el-api-*.jar
				</packagingExcludes>
			</configuration>
		</plugin>

		
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-deploy-plugin</artifactId>
			<version>2.7</version>
		</plugin>
		
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-source-plugin</artifactId>
			<version>2.1.2</version>
			<configuration></configuration>
		</plugin>
		
